CREATE TABLE `PowerTransformerPW` (`Num` varchar(255) DEFAULT NULL COMMENT '序号', `mRID` varchar(255) NOT NULL COMMENT '标识', `name` varchar(255) DEFAULT NULL COMMENT '名称', `rdfid` varchar(255) DEFAULT NULL COMMENT '资源标识', `type` varchar(255) DEFAULT NULL COMMENT '类型', `Substation` varchar(255) DEFAULT NULL COMMENT '所属厂站/站房', `ratedMVA` varchar(255) DEFAULT NULL COMMENT '额定容量', `I_BaseVoltage` varchar(255) DEFAULT NULL COMMENT '高端基准电压标识', `J_BaseVoltage` varchar(255) DEFAULT NULL COMMENT '低端基准电压标识', `I_node` varchar(255) DEFAULT NULL COMMENT '高端物理连接节点', `I_ratedMW` varchar(255) DEFAULT NULL COMMENT '高端额定功率', `I_ratedkV` varchar(255) DEFAULT NULL COMMENT '高端额定电压', `J_node` varchar(255) DEFAULT NULL COMMENT '低端物理连接节点', `J_ratedMW` varchar(255) DEFAULT NULL COMMENT '低端额定功率', `J_ratedkV` varchar(255) DEFAULT NULL COMMENT '低端额定电压', `Feeder` varchar(255) DEFAULT NULL COMMENT '所属馈线',  PRIMARY KEY (`mRID`)) ENGINE=InnoDB DEFAULT CHARSET=utf8;
